<p id="notice"><%= notice %></p>

<h1>Reviews</h1>

<table>
  <thead>
    <tr>
      <th>Puntctuation</th>
      <th>Puntctuation</th>
      <th>Efficacy</th>
      <th>Efficacy</th>
      <th>Behaviour</th>
      <th>Behaviour</th>
      <th>Commentary</th>
      <th>User</th>
      <th>Users speciality</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reviews.each do |review| %>
      <tr>
        <td><%= review.puntctuation %></td>
        <td><%= review.puntctuation %></td>
        <td><%= review.efficacy %></td>
        <td><%= review.efficacy %></td>
        <td><%= review.behaviour %></td>
        <td><%= review.behaviour %></td>
        <td><%= review.commentary %></td>
        <td><%= review.user %></td>
        <td><%= review.users_speciality %></td>
        <td><%= link_to 'Show', review %></td>
        <td><%= link_to 'Edit', edit_review_path(review) %></td>
        <td><%= link_to 'Destroy', review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Review', new_review_path %>
