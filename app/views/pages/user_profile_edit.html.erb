<%= simple_form_for @user, :url => user_profile_update_path, :method => :patch do |f|%>
  <%= f.input :email %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :phone_number %>
  <%= f.input :city %>
  <%= f.input :address, id: 'user_address'%>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>

  <%= f.submit %>

<% end %>

